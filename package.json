{
  "name": "@tailify/babel-preset-react",
  "version": "1.1.0",
  "description": "Tailify's very own opinionated Babel preset for React",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "npm run lint && npm run test-only",
    "test-only": "jest"
  },
  "repository": "tailify/babel-preset-react",
  "keywords": [
    "babel",
    "babel-preset"
  ],
  "license": "UNLICENSED",
  "bugs": "https://github.com/tailify/babel-preset-react/issues",
  "homepage": "https://github.com/tailify/babel-preset-react#readme",
  "dependencies": {
    "@babel/preset-react": "7.0.0",
    "@tailify/babel-preset": "2.1.0",
    "babel-plugin-react-css-modules": "5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@tailify/eslint-config-legacy": "^1.0.0",
    "@tailify/prettier-config": "^1.0.0",
    "eslint": "^5.9.0",
    "jest": "^23.6.0",
    "prettier": "^1.15.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  },
  "engines": {
    "node": ">=8"
  },
  "greenkeeper": {
    "commitMessages": {
      "dependencyUpdate": "Update ${dependency} to version ${version}",
      "devDependencyUpdate": "Update ${dependency} to version ${version}",
      "dependencyPin": "Pin ${dependency} to ${oldVersion}",
      "devDependencyPin": "Pin ${dependency} to ${oldVersion}"
    },
    "prTitles": {
      "basicPR": "Update ${dependency} to the latest version",
      "groupPR": "Update ${dependency} in group ${group} to the latest version"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
